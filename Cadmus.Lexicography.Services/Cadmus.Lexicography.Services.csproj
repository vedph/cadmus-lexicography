<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<ImplicitUsings>disable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<Authors>Daniele Fusi</Authors>
		<Company>Fusi</Company>
		<Product>Cadmus</Product>
		<Description>Services for Cadmus Lexicography.</Description>
		<Copyright>by Daniele Fusi 2025</Copyright>
		<NeutralLanguage>en-US</NeutralLanguage>
		<PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
		<PackageTags>Cadmus;parts</PackageTags>
		<Version>2.0.0</Version>
		<FileVersion>2.0.0</FileVersion>
		<AssemblyVersion>2.0.0</AssemblyVersion>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Cadmus.Core" Version="10.0.1" />
		<PackageReference Include="Cadmus.General.Parts" Version="8.0.1" />
		<PackageReference Include="Cadmus.Index.Sql" Version="10.0.1" />
		<PackageReference Include="Cadmus.Mongo" Version="10.0.1" />
		<PackageReference Include="Cadmus.Seed.General.Parts" Version="8.0.1" />
		<PackageReference Include="Cadmus.Seed.Philology.Parts" Version="12.0.1" />
		<PackageReference Include="Fusi.Microsoft.Extensions.Configuration.InMemoryJson" Version="5.0.0" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Cadmus.Lexicography.Parts\Cadmus.Lexicography.Parts.csproj" />
		<ProjectReference Include="..\Cadmus.Seed.Lexicography.Parts\Cadmus.Seed.Lexicography.Parts.csproj" />
	</ItemGroup>

</Project>
